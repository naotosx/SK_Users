{
  "welcomeFile": "/skc/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/skc/(.*)$",
      "target": "$1",
      "localDir": "../skc/dist",
      "authenticationType": "none"
    },
    {
      "source": "^/backend/(.*)$",
      "target": "$1",
      "destination": "POD_Claves",
      "authenticationType": "BasicAuthentication"
    },
    {
      "source": "^/api/soapQAS$",
      "target": "/XISOAPAdapter/MessageServlet?senderParty=&senderService=BS_SK_RCLAVES_Q&receiverParty=&receiverService=&interface=MI_mantenimiento_cliente_OS&interfaceNamespace=urn:skc.cl/mantenimiento_usuario_sap",
      "destination": "POQ_Claves",
      "csrfProtection": false
    },
    {
      "source": "^/backend3/(.*)$",
      "target": "$1",
      "destination": "POP_Claves",
      "authenticationType": "BasicAuthentication"
    }
  ]
}